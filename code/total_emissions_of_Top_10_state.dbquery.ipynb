   "source": [
    "select \n",
    "state_abbr,\n",
    "sum(cast(\n",
    "  replace(`GHG emissions mtons CO2e`, ',', '')\n",
    "  as double\n",
    ")) as total_emissions\n",
    "from dev_projects.databricks_data_vizualization.emissions_data\n",
    "group by state_abbr\n",
    "order by total_emissions desc\n",
    "limit 10"
   ]
  }
