{
  "datasets": [
    {
      "name": "df2beddd",
      "displayName": "location_Data",
      "queryLines": [
        "Select \n",
        "latitude,\n",
        "longitude,\n",
        "`GHG emissions mtons CO2e` as emissions\n",
        "from dev_projects.databricks_data_vizualization.emissions_data"
      ]
    },
    {
      "name": "044add83",
      "displayName": "emissions_per_person",
      "queryLines": [
        "select \n",
        "county_state_name,\n",
        "population,\n",
        "cast(\n",
        "  replace(`GHG emissions mtons CO2e`, ',', '')\n",
        "  as double\n",
        "  ) / nullif(\n",
        "    cast(\n",
        "    replace(population, ',', '')\n",
        "    AS double\n",
        "  ), 0\n",
        ") as emissions_per_person\n",
        "from dev_projects.databricks_data_vizualization.emissions_data\n",
        "order by emissions_per_person desc\n"
      ]
    },
    {
      "name": "628bae76",
      "displayName": "total_emissions_per_state",
      "queryLines": [
        "select \n",
        "state_abbr,\n",
        "sum(cast(\n",
        "  replace(`GHG emissions mtons CO2e`, ',', '')\n",
        "  as double\n",
        ")) as total_emissions\n",
        "from dev_projects.databricks_data_vizualization.emissions_data\n",
        "group by state_abbr\n",
        "order by total_emissions desc\n",
        "limit 10"
      ]
    },
    {
      "name": "68482797",
      "displayName": "county_shaming",
      "queryLines": [
        "select \n",
        "county_state_name,\n",
        "population,\n",
        "cast(replace(`GHG emissions mtons CO2e`, ',', '')as double) as total_emissions\n",
        "from dev_projects.databricks_data_vizualization.emissions_data\n",
        "order by total_emissions desc\n",
        "limit 10"
      ]
    }
  ],
